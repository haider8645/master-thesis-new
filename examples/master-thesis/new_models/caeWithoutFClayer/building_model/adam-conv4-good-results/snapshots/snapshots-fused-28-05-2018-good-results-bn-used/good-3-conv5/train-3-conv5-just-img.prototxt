#    stride: 2
#  }
#}
layer {
  name: "conv2_r"
  type: "Convolution"
  bottom: "conv1"
  top: "conv2_r"
  param { lr_mult: 1 decay_mult: 1 }
  param { lr_mult: 2 decay_mult: 0 }
  convolution_param {
    num_output: 128
    kernel_size: 3
    stride: 2
    weight_filler { type: "xavier" }
    bias_filler { type: "constant" value: 0 }
  }
}
layer {
  name: "reluconv2"
  type: "PReLU"
  bottom: "conv2_r"
  top: "conv2_r"
}
